from collections import defaultdict
positions = defaultdict(list)
n,m = map(int, input().split())
for i in range(1, n + 1):
    word = input().strip()
    positions[word].append(i)
for _ in range(m):
    word = input().strip()
    if word in positions:
        print(*positions[word])
    else:
        print(-1)
