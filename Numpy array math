import numpy
N,M = map(int, input().split())
A = []
B = []
for arrays in range(N):
    A.append(list(map(int, input().split())))
for arrays in range(N):
    B.append(list(map(int, input().split())))
A = numpy.array(A)
B = numpy.array(B)
print(A+B)
print(A-B)
print(A*B)
print(A//B)
print(A%B)
print(A**B)
